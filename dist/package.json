{
  "name": "ycshang-ui-vite",
  "version": "0.3.0",
  "description": "",
  "main": "ssy-ui.umd.js",
  "module": "ssy-ui.esm.js",
  "homepage": "https://ycshang-ui-vite.vercel.app/",
  "scripts": {
    "dev": "vite",
    "run:demo": "vite",
    "build": "pnpm build:components",
    "build:all": "vite build",
    "build:components": "esno ./scripts/build.ts",
    "lint": "eslint --fix --ext .ts,.tsx,.vue src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
    "docs:dev": "vitepress dev docs --config ../vite.config.ts",
    "docs:build": "vitepress build docs --config ../vite.config.ts",
    "docs:serve": "vitepress serve docs --config ../vite.config.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "ycshang",
  "license": "ISC",
  "dependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.2.6",
    "babel-eslint": "^10.1.0",
    "codecov": "^3.8.3",
    "eslint": "^8.29.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7",
    "prettier": "^2.8.1",
    "terser": "^5.16.1",
    "vite": "^3.2.5",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.3",
    "@iconify-json/ic": "^1.1.10",
    "@testing-library/vue": "^6.6.1",
    "@types/react": "^18.0.26",
    "@unocss/preset-attributify": "^0.35.4",
    "@unocss/preset-icons": "^0.30.13",
    "@unocss/preset-uno": "^0.35.4",
    "@vitest/ui": "^0.14.2",
    "c8": "^7.12.0",
    "happy-dom": "^4.1.0",
    "jsdom": "^19.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unocss": "^0.33.5",
    "vitepress": "^1.0.0-alpha.35",
    "vitepress-theme-demoblock": "^2.0.2",
    "vitest": "^0.14.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@vue/compiler-sfc",
        "typescript"
      ]
    }
  },
  "types": "ssy-ui.d.ts"
}